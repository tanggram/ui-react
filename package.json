{
  "homepage": "https://github.com/tanggram/ui-react-core",
  "name": "@tanggram/ui-react-core",
  "version": "0.3.2",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "@material-ui/lab": "^3.0.0-alpha.30",
    "@material-ui/styles": "^3.0.0-alpha.10",
    "classnames": "^2.2.6",
    "material-design-icons": "^3.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/plugin-transform-react-display-name": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "@types/enzyme": "^3.9.1",
    "codecov": "^3.2.0",
    "copywriter": "^0.1.0",
    "cross-env": "^5.2.0",
    "cross-spawn": "^6.0.5",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "enzyme-to-json": "^3.3.5",
    "express": "^4.16.4",
    "mock-local-storage": "^1.1.8",
    "react-test-renderer": "^16.8.6",
    "rollup": "^1.2.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-terser": "^4.0.4"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "mock-local-storage"
  },
  "keywords": [
    "react",
    "ui"
  ],
  "main": "index.js",
  "module": "esm/tanggram-ui-react-core.js",
  "private": false,
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "scripts": {
    "build": "npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:umd:min && bin/postbuild",
    "build:main": "npm run build:commonjs && npm run build:es && npm run build:post",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir build",
    "build:es": "babel src --out-dir build/es",
    "build:umd": "cross-env NODE_ENV=development rollup -c -o build/dist/tanggram-ui-react-core.js",
    "build:umd:min": "cross-env NODE_ENV=production rollup -c -o build/dist/tanggram-ui-react-core.min.js",
    "build:post": "bin/postbuild",
    "clean": "rimraf lib dist es coverage",
    "coverage": "codecov",
    "release": "npm build && npm publish build --tag next",
    "test": "echo \"Error: no test specified\" && exit 1"
  }
}
